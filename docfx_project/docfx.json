{
  "metadata": [
    {
      "src": [
        {
		  "src": "../",
          "files": [
            "**.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "Unity Example/**",
            "DiscordRPC.Example/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
	"sitemap":{
        "baseUrl": "https://lachee.github.io/discord-rpc-csharp/docs"
	},
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
		  "natives/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
	"globalMetadata": {
	  "_appFooter": "Â© 2019 Lachee"
	},
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}